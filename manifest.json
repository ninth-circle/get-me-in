{
  "manifest_version": 2,
  "name": "Get me in",
  "version": "1.0",
  "permissions": [
    "cookies",
    "activeTab",
    "storage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Get me in"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
